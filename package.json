{
  "name": "@halil-pan/ts-lib-template",
  "version": "1.2.2",
  "description": "awesome library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "xv dist",
    "lint": "xo",
    "start": "esno src/index.ts",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "husky install",
    "fresh": "npx taze -w & pnpm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halil-pan/ts-lib-template.git"
  },
  "keywords": [
    "typescript",
    "library"
  ],
  "author": "halil-pan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halil-pan/ts-lib-template/issues"
  },
  "homepage": "https://github.com/halil-pan/ts-lib-template#readme",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node14": "^1.0.3",
    "@types/node": "^18.11.19",
    "esno": "^0.12.1",
    "husky": "^7.0.4",
    "rome": "^11.0.0",
    "tsup": "^5.12.9",
    "typescript": "^4.9.5",
    "xo": "0.46.4",
    "xv": "^1.1.1"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.test.*.map",
    "!dist/**/*.test.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  }
}
